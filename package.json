{
  "name": "enhance",
	"displayName": "enhance",
	"description": "When you need to make your text just a little bit bigger, but CMD + is TOO MUCH WORK",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:enhance.sayWassup",
        "onCommand:enhance.enhance",
        "onCommand:enhance.unenhance",
        "onCommand:enhance.omega-enhance"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
    {
      "command": "enhance.sayWassup",
      "title": "Hello World"
    },
    {
      "command": "enhance.enhance",
      "title": "Enhance: ENHANCE!"
    },
    {
      "command": "enhance.omega-enhance",
      "title": "Enhance: OMEGA ENHANCE"
    },
    {
      "command": "enhance.unenhance",
      "title": "Enhance: Unenhance"
    }
  ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.52.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    }

}
